{
    "name": "marcwiest/primera-package",
    "description": "The package of the Primera WordPress theme.",
    "version": "1.0.0-beta",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Marc Wiest",
            "email": "marc@marcwiest.com"
        }
    ],
    "keywords": [
        "wordpress", "wp", "woocommerce", "blade", "MVC"
    ],
    "require": {
        "php": "^7.2",
        "brain/hierarchy": "2.4",
        "duncan3dc/blade": "4.8",
        "soberwp/controller": "2.1.1",
        "symfony/var-dumper": "4.3.5",
        "vlucas/phpdotenv": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "Primera\\": "src/"
        },
        "files": [
            "index.php"
        ]
    },
    "config": {
        "sort-packages": true
    }
}
